@model List<Chefs>
<!-- ======= Chefs Section ======= -->
<section id="chefs" class="chefs">
    <div class="container" data-aos="fade-up">

        <div class="section-title">
            <h2>Chefs</h2>
            <p>Our Proffesional Chefs</p>
        </div>

        <div class="row">

            @foreach (var chef in Model)
            {
                <div class="col-lg-4 col-md-6">
                    <div class="member" data-aos="zoom-in" data-aos-delay="100">
                        <img src="assets/img/chefs/@chef.Image" class="img-fluid" alt="">
                        <div class="member-info">
                            <div class="member-info-content">
                                <h4>@chef.Name</h4>
                                <span>@chef.Professions.Name</span>
                            </div>
                            <div class="social">
                                <a href="@chef.TwitterAddress"><i class="bi bi-twitter"></i></a>
                                <a href="@chef.FacebookAddress"><i class="bi bi-facebook"></i></a>
                                <a href="@chef.InstagramAddress"><i class="bi bi-instagram"></i></a>
                                <a href="@chef.LinkedinAddressId"><i class="bi bi-linkedin"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            }

           

        </div>

    </div>
</section><!-- End Chefs Section -->
