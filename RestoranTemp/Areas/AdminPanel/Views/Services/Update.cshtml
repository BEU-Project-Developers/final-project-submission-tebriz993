@using RestoranTemp.Models;
@model Services

<div class="col-lg-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create</h4>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label asp-for="Title">
                        Title
                    </label>
                    <input asp-for="Title" type="text" class="form-control" placeholder="Title" />
                    <span asp-validation-for="Title"></span>
                </div>

                <div class="form-group">
                    <label asp-for="Subtitle">
                        Title
                    </label>
                    <input asp-for="Subtitle" type="text" class="form-control" placeholder="Subtitle" />
                    <span asp-validation-for="Subtitle"></span>
                </div>

                <button type="submit" class="btn btn-primary mr-2">Submit</button>
                <button class="btn btn-light">Cancel</button>

            </form>
        </div>
    </div>
</div>